<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skate.android.slandit_skate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/skate_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        

        <activity android:name=".LogInform"></activity>
        <activity android:name=".SignUpForm"></activity>
        <activity android:name=".UserProfile"></activity>
        <activity android:name="com.example.android.slandit_skate.Home"></activity>
        <activity android:name="com.example.android.slandit_skate.SearchData"></activity>
        <activity android:name="com.example.android.slandit_skate.Message"></activity>
        <activity android:name="com.example.android.slandit_skate.Notification"></activity>
    </application>

</manifest>